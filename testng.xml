<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="V2 Regression Tests">
    <parameter name="browser" value="${browser}"/>
    <parameter name="appURL" value="${appURL}"/>

    <test name="Embold Sign Up Validation">
        <classes>
            <class name="BeginTestExecution"/>
            <class name="EmboldSignUpPageTests"/>
            <class name="EndTestExecution"/>
        </classes>
    </test>

    <test name="Embold Sign In Validation">
        <classes>
            <class name="BeginTestExecution"/>
            <class name="EmboldSignInPageTests"/>
            <class name="EndTestExecution"/>
        </classes>
    </test>

    <test name="Embold Sign out Validation">
        <classes>
            <class name="BeginTestExecution"/>
            <class name="EmboldSignOutTests"/>
            <class name="EndTestExecution"/>
        </classes>
    </test>
</suite>