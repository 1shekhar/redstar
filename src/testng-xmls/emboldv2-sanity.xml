<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="V2 Regression Tests">
    <parameter name="browser" value="${browser}"/>
    <parameter name="appURL" value="${appURL}"/>
    <!--EmboldSignOutTests are exclude for time being since pew sanity tests have already created SCM sessions.
    SignOut tests need clear session. We will handle this later.
    <class name="EmboldSignOutTests"/>-->

    <test name="Embold V2 Sanity">
        <classes>
            <class name="BeginTestExecution"/>
            <class name="EmboldSignUpPageTests"/>
            <class name="EmboldSignInPageTests"/>
            <class name="EmboldSignOutTests"/>
            <class name="EndTestExecution"/>
        </classes>
    </test>
</suite>